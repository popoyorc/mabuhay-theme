<?xml version="1.0"?>
<!DOCTYPE look-and-feel PUBLIC "-//Liferay//DTD Look and Feel 7.0.0//EN" "http://www.liferay.com/dtd/liferay-look-and-feel_7_0_0.dtd">

<look-and-feel>
	<compatibility>
		<version>7.0.0+</version>
	</compatibility>
	<theme id="parent-theme" name="parent-theme">
		<template-extension>vm</template-extension>
		<settings>
			<setting configurable="false" key="show-breadcrumb" type="checkbox" value="false" />
			<setting configurable="true" key="parent-setting" type="input" value="true" />
		</settings>
		<portlet-decorator id="portlet-decorator-1" name="Decorator 1">
			<portlet-decorator-css-class>portlet-decorator-1-from-parent</portlet-decorator-css-class>
		</portlet-decorator>
		<portlet-decorator id="portlet-decorator-3" name="Decorator 3">
			<portlet-decorator-css-class>portlet-decorator-3</portlet-decorator-css-class>
		</portlet-decorator>
	</theme>
</look-and-feel>